{{ for set, images of @sets :}}

  <section>
    <h2>Test set: {{- set }}</h2>

    {{ for img, i in images : }}
      {{ if img.measurement.saliency.confidence < 0.50 : }}
      <subsection>
        <img src="../{{-img.image}}" />
        <img src="../{{-img.image}}_saliency.png" />
        <img src="../{{-img.image}}_threshold.png" />
        <img src="../{{-img.image}}_filtered.png" />
        <img src="../{{-img.image}}_contours.png" />
        <img src="../{{-img.image}}_histogram_saliency.png" /><br/>
        <caption>({{- i}}) Confidence: <b>{{- img.measurement.saliency.confidence}}</b>. Test set: {{- set}}. File path: {{- img.image}}.</caption>
      </subsection><br /><br />
      {{ end }}
    {{ end }}

  </section>

{{ end }}